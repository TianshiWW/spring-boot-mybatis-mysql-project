<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.spring.project.springboot.dao.UserDao">
    <select id="getAllUser" resultType="com.spring.project.springboot.model.Employee" >
        SELECT * FROM employees;
    </select>

    <insert id="insertUser" parameterType="com.spring.project.springboot.model.Employee"  useGeneratedKeys="true" keyProperty="id">
        insert into
            employees(id, firstName, lastName, emailAddress)
        values
            (#{id},#{firstName},#{lastName},#{emailAddress})
    </insert>
</mapper>
